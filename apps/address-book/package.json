{
  "name": "@tpt/apps-address-book",
  "version": "0.0.1",
  "description": "Addressbook registry app from Planning Suite",
  "dependencies": {
    "@aragon/client": "^1.0.0-beta.9",
    "@aragon/os": "^3.1.9",
    "@aragon/ui": "^0.16.0",
    "babel-runtime": "^6.26.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styled-components": "^3.4.2"
  },
  "devDependencies": {
    "@aragon/cli": "^4.1.5",
    "ajv": "^6.5.2",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eth-gas-reporter": "^0.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "react-hot-loader": "^4.3.4",
    "solidity-coverage": "0.3.5",
    "solium": "^1.1.8",
    "truffle": "4.0.5",
    "truffle-hdwallet-provider": "^0.0.6"
  },
  "scripts": {
    "build:app": "npx parcel build app/index.html -d dist/ --public-url '.'",
    "build:script": "npx parcel build app/script.js -d dist/",
    "build": "npm run compile && npm run sync-assets && npm run build:app && npm run build:script",
    "compile": "npx truffle compile",
    "console": "npx truffle dev",
    "coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "dev": "npm run sync-assets && npx parcel app/index.html --open",
    "devchain": "npx aragon devchain",
    "ganache-cli:test": "../shared/test-helpers/ganache-cli.sh",
    "lint": "npx solium --dir ./contracts",
    "prepublishOnly": "npx truffle compile --all",
    "publish": "npx apm publish",
    "start": "npx aragon run",
    "sync-assets": "npx copy-aragon-ui-assets ./dist",
    "test:gas": "GAS_REPORTER=true npm test",
    "test": "TRUFFLE_TEST=true npm run ganache-cli:test",
    "link:shared": "npm link ../shared/test-helpers"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Giveth/planning-app"
  },
  "license": "GPL-3.0"
}
