  "scripts": {
    "build:app": "npx parcel build app/index.html -d dist/ --public-url '.'",
    "build:script": "npx parcel build app/script.js -d dist/",
    "build": "npm run compile && npm run sync-assets && npm run build:app && npm run build:script",
    "compile": "npx aragon contracts compile",
    "dev": "npm run sync-assets && npx parcel src/index.html --open",
    "devchain": "npx aragon devchain",
    "lint:css": "npx stylelint './src/**/*.js'",
    "publish": "npx apm publish",
    "start": "npx aragon run",
    "sync-assets": "npx copy-aragon-ui-assets ./dist",
    "test": "npx aragon contracts test"
  },
