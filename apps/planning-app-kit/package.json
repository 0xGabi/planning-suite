{
  "name": "@tpt/kits-planning-app",
  "version": "0.0.1",
  "description": "Aragon planning suite developed by Space Decentral and Giveth.",
  "scripts": {
    "coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "link:aragon": "mkdir -p node_modules/@aragon/os && ln -s $PWD/../../node_modules/@aragon/os/contracts node_modules/@aragon/os/contracts",
    "lint": "solium --dir ./contracts",
    "start:kit": "aragon run --kit PlanningKit --kit-init @ARAGON_ENS",
    "ganache-cli:test": "../shared/test-helpers/ganache-cli.sh",
    "test:gas": "GAS_REPORTER=true npm test",
    "test": "TRUFFLE_TEST=true npm run ganache-cli:test"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Giveth/planning-app"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "@tpt/apps-address-book": "^0.0.1",
    "@tpt/apps-allocations": "^0.0.1",
    "@tpt/apps-projects": "^0.0.1",
    "@tpt/apps-range-voting": "^0.0.1"
  }
}
